{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    // Remove the allowImportingTsExtensions option
    // "allowImportingTsExtensions": true,
    "allowJs": false, // Set to false if your project is mostly TypeScript
    "checkJs": false, // Set to false if your project is mostly TypeScript
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}
